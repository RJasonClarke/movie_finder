<h1>Your lists</h1>


<% @lists.each do |list| %>
<ul>

    <li><a href="/lists/<%= list.id %>"><%= list.name %></a></li>
    
    
    <a href='/lists/<%=list.id%>/edit'><button>Edit</button></a>


    <form action="/lists/<%= list.id %>" method="post">
    <input id='hidden' type="hidden" name="_method" value="DELETE">
    <input type="submit" value="Delete">
    </form>

</ul>

<% end %>

